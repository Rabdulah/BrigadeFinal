<div id='content'>
    <h2> Question List </h2>
    <div class="ui grid">
    {{#each model as |question|}}   
        <div class="eight wide column">
        <li>{{question.questionText}} </li>
        </div>
        <div class="four wide column right aligned">
        {{modify-question ID=question.id}}
        </div>
        <div class="four wide column left aligned">
        {{delete-question ID=question.id}}
        </div>
    {{/each}}
    </div>
</div>
